
<% 
if(this.tutor === undefined){
    this.tutor = {
        nome: "",
        login: "",
        senha: "",
        email: ""
    }
}
%>
<div class="campos-form">
    <div><label for="nome">Nome:</label><input type="text" value="<%= tutor.nome %>" name="nome" id="nome"></div>
    <div><label for="login">Login:</label> <input type="text" value="<%= tutor.login %>" name="login" id="login"></div>
    <div><label for="senha">Senha:</label> <input type="text" value="<%= tutor.senha %>" name="senha" id="senha"></div>
    <div><label for="email">E-mail:</label> <input type="text" value="<%= tutor.email %>" name="email" id="email"></div>
</div>